struct Person {
    string name :: ""
    string surname :: ""
    int age :: 0

    func full_name :: () string {
        return name + " " + surname;
    }
}

func is_major :: (Person person) string |
    if person.age > 18 then 'Major' else 'Minor' 


Person daniel = {
    name :: "Daniel"
    surname :: "Steph"
}

print daniel.full_name
print is_major(daniel)

bool correct :: is_major(daniel) & age < 10